if [ $OPTION_PET_DB_NAME = 'pet-east-northants' ]; then
    START_DATE=2010-12-13
elif [ $OPTION_PET_DB_NAME = 'pet-hounslow' ]; then
    START_DATE=2010-11-01
elif [ $OPTION_PET_DB_NAME = 'pet-lichfield' ]; then
    START_DATE=2010-12-13
elif [ $OPTION_PET_DB_NAME = 'pet-newforest' ]; then
    START_DATE=2010-12-07
elif [ $OPTION_PET_DB_NAME = 'pet-stedmundsbury' ]; then
    START_DATE=2010-12-11
elif [ $OPTION_PET_DB_NAME = 'pet-surrey' ]; then
    START_DATE=2010-03-01
elif [ $OPTION_PET_DB_NAME = 'pet-sbdc' ]; then
    START_DATE=2010-02-01
elif [ $OPTION_PET_DB_NAME = 'pet-waveney' ]; then
    START_DATE=2010-12-08
elif [ $OPTION_PET_DB_NAME = 'pet-westminster' ]; then
    START_DATE=2010-12-13
else # Number 10/ default
    START_DATE=2006-11-14
fi

